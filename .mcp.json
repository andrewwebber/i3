{
  "mcpServers": {

     "youtube": {
         "command": "npx",
         "args": ["@anaisbetts/mcp-youtube"]
      },
     "fetch": {
         "command": "uvx",
         "args": [
           "mcp-server-fetch"
         ]
      },
     "time": {
         "command": "uvx",
         "args": [
           "mcp-server-time",
           "--local-timezone",
           "Europe/Berlin"
         ]
      }
  }
}
